    "@capacitor/core": "^3.5.1",
    "@capacitor/toast": "^1.0.8",
    "@robingenz/capacitor-android-dark-mode-support": "^1.0.0",
    "redaxios": "^0.5.0",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.5.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "cordova-res": "^0.15.4",
    "jetifier": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-windicss": "^1.8.4",
    "vue-tsc": "^0.34.15",
    "windicss": "^3.5.4"
  }
}
