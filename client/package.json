    "gradle": "cd android && ./gradlew assembleDebug",
    "resources": "cordova-res android --skip-config --copy"
  },
  "dependencies": {
    "@babel/types": "^7.21.3",
    "@capacitor-community/wifi": "github:mirsella/capacitor4-wifimanager",
    "@capacitor/android": "^4.7.1",
    "@capacitor/clipboard": "^4.1.0",
    "@capacitor/core": "^4.7.1",
    "@capacitor/splash-screen": "^4.2.0",
    "redaxios": "^0.5.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.7.1",
    "@vitejs/plugin-vue": "^2.3.4",
    "cordova-res": "^0.15.4",
    "jetifier": "^2.0.0",
    "typescript": "^5.0.2",
    "vite": "^2.9.14",
    "vite-plugin-windicss": "^1.8.10",
    "vue-tsc": "^1.2.0",
    "windicss": "^3.5.6"
  }
}
