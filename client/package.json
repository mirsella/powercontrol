{
  "name": "powercontrol-client",
  "version": "7.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "android": "vue-tsc --noEmit && vite build && cap run android",
