{
  "name": "powercontrol-client",
  "version": "6.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "android": "vue-tsc --noEmit && vite build && cap run android",
